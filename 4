#include<bits/stdc++.h>
using namespace std;

void evn(stack<int>&st){
   
    while(!st.empty()){
        int k=st.top();
        if(k%2==0){
            cout<<k<<" ";
        }
       
        st.pop();
        
    }
    
    
}

int main(){
    stack<int>st;
   
    for(int i=1;i<=10;i++){
        
        st.push(i);
    }
    
    evn(st);
   
}